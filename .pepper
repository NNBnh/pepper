config indent_with_tabs false

alias build [[enqueue-keys ":o build.out scratch<enter>aa!cmd /c cargo build 2<greater>&1<enter>"]]
alias test [[enqueue-keys ":o test.out scratch<enter>aa!cmd /c cargo test 2<greater>&1<enter>"]]

map-normal <space>b ": build<enter>"
map-normal <space>t ": test<enter>"

